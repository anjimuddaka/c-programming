#include<stdio.h>
void prime(int n);
//int prime(int n)
void main()
{

    int a;
    printf("enter the range");
    scanf("%d",&a);
    prime(a);
}
void prime(int n)
{
    if(n!=1)
{


    int count=0;
    for(int i=2;i<n;i++)
    {
        if(n%i==0)
            count=1;
    }
    if(count==0)
    {
        printf("%d\n",n);
    }
prime(n-1);
}
}
